<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="203.0" prefWidth="429.0"
            xmlns="http://javafx.com/javafx/8.0.171" fx:controller="controller.LoginFormController">
   <children>
      <JFXTextField fx:id="txtName" layoutX="72.0" layoutY="91.0" prefHeight="29.0" prefWidth="286.0"
                    unFocusColor="#866262"/>
      <Text fill="#9b329d" layoutX="155.0" layoutY="88.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Your Name">
         <font>
            <Font name="Rockwell Bold" size="14.0"/>
         </font>
      </Text>
      <JFXButton layoutX="177.0" layoutY="146.0" onAction="#logInButtonOnAction" prefHeight="36.0" prefWidth="74.0"
                 style="-fx-background-radius: 20; -fx-background-color: #000000;" text="LogIn" textFill="WHITE">
         <font>
            <Font name="Segoe Script Bold" size="14.0"/>
         </font>
      </JFXButton>
      <ImageView fitHeight="50.0" fitWidth="62.0" layoutX="185.0" layoutY="14.0" pickOnBounds="true"
                 preserveRatio="true">
         <image>
            <Image url="@../img/logo.png"/>
         </image>
      </ImageView>
   </children>
</AnchorPane>
