<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="pane" maxWidth="331.0" minWidth="331.0" prefHeight="528.0"
            prefWidth="331.0" xmlns="http://javafx.com/javafx/8.0.171">
    <children>
        <ScrollPane fx:id="scrollPain" layoutY="61.0" prefHeight="406.0" prefWidth="331.0">
            <content>
                <VBox fx:id="vBox" prefHeight="391.0" prefWidth="312.0"/>
            </content>
        </ScrollPane>
        <JFXTextField fx:id="txtMsg" layoutX="30.0" layoutY="480.0" onAction="#txtMsgOnAction" prefHeight="26.0"
                      prefWidth="212.0"/>
        <JFXButton contentDisplay="GRAPHIC_ONLY" layoutX="284.0" layoutY="481.0" onAction="#sendButtonOnAction"
                   prefHeight="26.0" prefWidth="29.0">
            <graphic>
                <ImageView fitHeight="19.0" fitWidth="19.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../img/send-icon.png"/>
                    </image>
                </ImageView>
            </graphic>
        </JFXButton>
        <Text fx:id="txtLabel" fill="#b31ece" layoutX="248.0" layoutY="38.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="Client">
            <font>
                <Font name="Calibri Bold" size="20.0"/>
            </font>
        </Text>
        <JFXButton fx:id="emojiButton" contentDisplay="GRAPHIC_ONLY" layoutX="247.0" layoutY="476.0" prefHeight="34.0"
                   prefWidth="37.0" text="Button">
            <graphic>
                <ImageView fitHeight="26.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../img/happy-face.png"/>
                    </image>
                </ImageView>
            </graphic>
        </JFXButton>
        <JFXButton contentDisplay="GRAPHIC_ONLY" layoutX="6.0" layoutY="476.0" onAction="#attachedButtonOnAction"
                   prefHeight="34.0" prefWidth="18.0" text="Button">
            <graphic>
                <ImageView fitHeight="18.0" fitWidth="17.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../img/attachment-icon.png"/>
                    </image>
                </ImageView>
            </graphic>
        </JFXButton>
        <ImageView fitHeight="46.0" fitWidth="54.0" layoutX="4.0" layoutY="8.0">
            <image>
                <Image url="@../img/logo.png"/>
            </image>
        </ImageView>
    </children>
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
</AnchorPane>
